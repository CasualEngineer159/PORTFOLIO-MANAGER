# Dynamické proměnné
base_url = 'https://www.alphavantage.co/query'
function = 'OVERVIEW'
symbol = 'IBM'
apikey = '5H3BQBCDJJJ9TTFU'

# Složení URL
params = {
    'function': function,
    'symbol': symbol,
    'apikey': apikey
}

# Dotaz na API
r = requests.get(base_url, params=params)
data = r.json()

pp.pprint(data)